
doctor.jersey.http {
    bind: "localhost:9998,localhost:9999"
    tcp {
        threads: 1
        threadFormat: netty-jersey-tcp-%d
        socketBacklog: 1024
    }
    worker {
        threads: 16
        threadFormat: netty-jersey-worker-%d
    }
    ssl {
        selfSigned: false
        keyCertChainFile: ""
        keyFile: ""
        keyPassword: ""
    }
    maxInitialLineLength: 8192
    maxHeaderSize: 8192
    maxChunkSize: 8192
    validateHeaders: false
    initialBufferSize: 8192
    maxContentLength: 8388608
    minGzipSize: 812
}